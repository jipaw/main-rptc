{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules",
    "bower_components/"
  ],
  "verbose": true,
  "execMap": {
    "js": "node --harmony"
  },
  "watch": [
    "src/",
    "config/",
    "public/"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js json html css"
}

